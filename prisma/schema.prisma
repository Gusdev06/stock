// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgres"
  url      = env("DATABASE_URL")
}


model Product {
  id          Int         @id @default(autoincrement())
  guid        String      @unique @default(uuid())
  name        String
  description String
  price       Float
  createdAt   DateTime
  updatedAt   DateTime
}

